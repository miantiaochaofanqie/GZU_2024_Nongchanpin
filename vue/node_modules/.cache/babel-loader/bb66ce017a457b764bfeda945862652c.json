AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,GAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAHA,EAGA,KAHA,CAGA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OALA;AAMA,KAXA;AAYA,IAAA,UAZA,sBAYA,IAZA,EAYA;AAAA;;AACA,WAAA,QAAA,CAAA,WAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,kBAAA,CAAA;AAAA,UAAA,EAAA,EAAA,IAAA,CAAA;AAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;;AAIA,UAAA,MAAA,CAAA,OAAA;AACA,SANA,EAMA,KANA,CAMA,UAAA,GAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,SARA;AASA,OAdA,EAcA,KAdA,CAcA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,MADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAAA;AAIA,OAnBA;AAoBA,KAjCA;AAkCA,IAAA,YAlCA,wBAkCA,IAlCA,EAkCA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,oBAAA,IAAA,EAAA;AACA,KArCA;AAsCA,IAAA,WAtCA,yBAsCA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KAxCA;AAyCA,IAAA,UAzCA,sBAyCA,IAzCA,EAyCA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,oBAAA,IAAA,EAAA;AACA,KA5CA;AA6CA,IAAA,WA7CA,yBA6CA;AACA,WAAA,aAAA,GAAA,KAAA;AACA,KA/CA;AAgDA,IAAA,YAhDA,0BAgDA;AAAA;;AACA,WAAA,SAAA,CAAA,MAAA,GAAA,CAAA;AACA,MAAA,qBAAA,CAAA,KAAA,SAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA;AACA,OAJA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OANA;AAOA;AAzDA,GAfA;AA0EA,EAAA,OA1EA,qBA0EA;AACA,SAAA,MAAA,CAAA,MAAA,CAAA,uBAAA,EAAA,KAAA;AACA,SAAA,OAAA;AACA;AA7EA,CAAA","sourcesContent":["<!--专家问答-->\r\n<template>\r\n  <div class=\"expert-appoint-container\">\r\n    <div v-for=\"(item,index) in appointArray\" :key=\"index\" class=\"appoint-item\">\r\n      <div class=\"appoint-content\">\r\n        <div class=\"title\">{{item.plantName}}</div>\r\n        <div class=\"content\">{{item.plantDetail}}</div>\r\n        <div class=\"item-container\">\r\n          <div class=\"item-content marginR30\" v-if=\"role==='expert'\">\r\n            <div class=\"item-title\">咨询者：</div>\r\n            <div class=\"item-text\">{{item.questioner}}</div>\r\n          </div>\r\n          <div class=\"item-content marginR30\" v-if=\"role==='expert'\">\r\n            <div class=\"item-title\">咨询者联系方式：</div>\r\n            <div class=\"item-text\">{{item.phone}}</div>\r\n          </div>\r\n          <div class=\"item-content marginR30\" v-if=\"role==='questioner'\">\r\n            <div class=\"item-title\">专家姓名：</div>\r\n            <span class=\"item-text\">{{item.expertName}}</span>\r\n          </div>\r\n        </div>\r\n        <div class=\"item-container\">\r\n          <div class=\"item-content marginR30\">\r\n            <div class=\"item-title\">作物条件：</div>\r\n            <span class=\"item-text\">{{item.plantCondition}}</span>\r\n          </div>\r\n          <div class=\"item-content marginR30\">\r\n            <div class=\"item-title\">土壤条件：</div>\r\n            <div class=\"item-text\" :title=\"item.soilCondition\">{{item.soilCondition}}</div>\r\n          </div>\r\n          <div class=\"item-content marginR30\">\r\n            <div class=\"item-title\">面积：</div>\r\n            <div class=\"item-text\">{{item.area}}亩</div>\r\n          </div>\r\n          <el-tag style=\"position:relative;left:360px\" :type=\"item.status === 0 ? 'danger':'success'\" size=\"mini\">{{item.status === 0 ? '未回答' :'已回答'}}</el-tag>\r\n        </div>\r\n      </div>\r\n      <div class=\"appoint-btn\">\r\n        <div class=\"btn-text\" @click=\"handleDetail(item)\">详情</div>\r\n        <div class=\"btn-text\" @click=\"handleEdit(item)\">修改</div>\r\n        <div class=\"btn-text\" @click=\"delAppoint(item)\">删除</div>\r\n      </div>\r\n    </div>\r\n    <el-dialog title=\"详情\" :visible.sync=\"showDetail\" width=\"600px\" :before-close=\"detailClose\">\r\n      <div class=\"detail-content\">\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">种植作物：</div>\r\n          <div class=\"item-content\">{{detailObj.plantName}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">作物详细信息：</div>\r\n          <div class=\"item-content\">{{detailObj.plantDetail}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">地址：</div>\r\n          <div class=\"item-content\">{{detailObj.address}}</div>\r\n        </div>\r\n         <div class=\"detail-item\">\r\n          <div class=\"item-title\">土壤条件：</div>\r\n          <div class=\"item-content\">{{detailObj.soilCondition}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">面积：</div>\r\n          <div class=\"item-content\">{{detailObj.area}}</div>\r\n        </div>\r\n         <div class=\"detail-item\">\r\n          <div class=\"item-title\">作物条件：</div>\r\n          <div class=\"item-content\">{{detailObj.plantCondition}}</div>\r\n        </div>\r\n         <div class=\"detail-item\" v-if=\"role==='expert'\">\r\n          <div class=\"item-title\">联系方式：</div>\r\n          <div class=\"item-content\">{{detailObj.phone}}</div>\r\n        </div>\r\n         <div class=\"detail-item\" v-if=\"role==='expert'\">\r\n          <div class=\"item-title\">提问者：</div>\r\n          <div class=\"item-content\">{{detailObj.questioner}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">问题状态：</div>\r\n          <el-tag type=\"danger\" size=\"mini\">{{detailObj.status === 0 ? '未回答' :'已回答'}}</el-tag>\r\n        </div>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"showDetail = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"showDetail = false\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n    <el-dialog title=\"修改\" :visible.sync=\"dialogVisible\" width=\"650px\" :before-close=\"closeRevise\">\r\n      <div class=\"detail-content\">\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">种植作物：</div>\r\n          <div class=\"item-content\">{{detailObj.plantName}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">作物详细信息：</div>\r\n          <div class=\"item-content\">{{detailObj.plantDetail}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">地址：</div>\r\n          <div class=\"item-content\">{{detailObj.address}}</div>\r\n        </div>\r\n         <div class=\"detail-item\">\r\n          <div class=\"item-title\">土壤条件：</div>\r\n          <div class=\"item-content\">{{detailObj.soilCondition}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">面积：</div>\r\n          <div class=\"item-content\">{{detailObj.area}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">作物条件：</div>\r\n          <div class=\"item-content\">{{detailObj.plantCondition}}</div>\r\n        </div>\r\n         <div class=\"detail-item\">\r\n          <div class=\"item-title\">联系方式：</div>\r\n          <div class=\"item-content\">{{detailObj.phone}}</div>\r\n        </div>\r\n         <div class=\"detail-item\">\r\n          <div class=\"item-title\">提问者：</div>\r\n          <div class=\"item-content\">{{detailObj.questioner}}</div>\r\n        </div>\r\n        <div class=\"detail-item\">\r\n          <div class=\"item-title\">问题状态：</div>\r\n          <el-tag type=\"danger\" size=\"mini\">{{detailObj.status === 0 ? '未回答' :'已回答'}}</el-tag>\r\n        </div>\r\n        <el-form ref=\"form\" :model=\"detailObj\" label-width=\"60px\">\r\n          <el-form-item label=\"回答：\">\r\n            <el-input type=\"textarea\" v-model=\"detailObj.answer\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"closeRevise\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitRevise\">确 定</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { selectAppointByUser,reviseAppointByUserId,delAppointByUserId } from '../../../api/question.js'\r\n\r\nexport default {\r\n  data(){\r\n    return{\r\n      appointArray:[],\r\n      role:\"\",\r\n      showDetail: false,\r\n      dialogVisible: false,\r\n      detailObj:{\r\n        title:'',\r\n        expertName:'',\r\n        questioner:'',\r\n        status:''\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    getData(){\r\n      this.role =  this.$store.getters.isExpert?'expert':'questioner'\r\n\r\n      // kind：普通用户：questioner；专家：expert\r\n      selectAppointByUser({type:this.role}).then(res => {\r\n        console.log('rererer',res)\r\n        this.appointArray = res.data\r\n      }).catch(err=>{\r\n        console.log(err)\r\n      })\r\n    },\r\n    delAppoint(item){\r\n      this.$confirm('确认删除该行信息？', '删除', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        delAppointByUserId({id:item.id}).then(res=>{\r\n          this.$message({\r\n            type: 'success',\r\n            message: '删除成功!'\r\n          });\r\n          this.getData()\r\n        }).catch(err=>{\r\n          console.log(err)\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        });\r\n      });\r\n    },\r\n    handleDetail(item){\r\n      this.showDetail = true\r\n      this.detailObj = Object.assign({},{...item})\r\n    },\r\n    detailClose(){\r\n      this.showDetail = false\r\n    },\r\n    handleEdit(item){\r\n      this.dialogVisible = true\r\n      this.detailObj = Object.assign({},{...item})\r\n    },\r\n    closeRevise(){\r\n      this.dialogVisible = false\r\n    },\r\n    submitRevise(){\r\n      this.detailObj.status = 1\r\n      reviseAppointByUserId(this.detailObj).then(res => {\r\n        this.$message.success('修改成功！')\r\n        this.dialogVisible = false\r\n        this.getData()\r\n      }).catch(err=>{\r\n        console.log(err)\r\n      })\r\n    }\r\n  },\r\n  mounted(){\r\n    this.$store.commit(\"updateUserActiveIndex\", \"4-2\");\r\n    this.getData()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.expert-appoint-container{\r\n  width: 900px;\r\n  min-height: 100%;\r\n  // padding: 10px 20px;\r\n  background: #fff;\r\n  .appoint-item{\r\n    border:1px solid #f2f2f2;\r\n    border-radius: 6px;\r\n    padding: 10px 20px;\r\n    margin: 10px 20px;\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    .appoint-content{\r\n      width: 650px;\r\n      .title{\r\n        font-weight: bold;\r\n        font-size: 18px;\r\n        line-height: 30px;\r\n        height: 30px;\r\n      }\r\n      .content{\r\n        line-height: 25px;\r\n        height: auto;\r\n        word-break: break-all;\r\n      }\r\n      .item-container{\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: flex-start;\r\n        align-items: center;\r\n        font-size: 12px;\r\n        line-height: 20px;\r\n        .item-content{\r\n          display: flex;\r\n          flex-direction: row;\r\n          justify-content: flex-start;\r\n          align-items: center;\r\n        }\r\n        .item-title{\r\n          max-width: 100px;\r\n        }\r\n      }\r\n      .marginR30{\r\n        margin-right: 30px;\r\n      }\r\n      .item-text{\r\n        color: #666;\r\n        max-width: 200px;\r\n        /*超出的部分隐藏*/\r\n        overflow: hidden;\r\n        /*文字用省略号替代超出的部分*/\r\n        text-overflow: ellipsis;\r\n        /*弹性伸缩盒子模型显示*/\r\n        display: -webkit-box;\r\n        /*限制在一个块元素显示文本的行数*/\r\n        -webkit-line-clamp: 1;\r\n        /*设置或检索伸缩盒对象的子元素排列方式*/\r\n        -webkit-box-orient: vertical;\r\n        word-break: break-all;\r\n      }\r\n    }\r\n    .appoint-btn{\r\n      display: flex;\r\n      .btn-text{\r\n        height: 25px;\r\n        cursor: pointer;\r\n        margin-right: 10px;\r\n        color: #67C23A;\r\n        &:hover{\r\n          color: #035D1C;\r\n          text-decoration: underline;\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .detail-content{\r\n    max-height: 500px;\r\n    height: auto;\r\n    overflow-y: auto;\r\n  }\r\n  .detail-item{\r\n    display: flex;\r\n    line-height: 30px;\r\n    .item-content{\r\n      line-height: 30px;\r\n      height: auto;\r\n      width: 480px;\r\n      display: flex;\r\n    }\r\n    .item-title{\r\n      width: 150px;\r\n      height: 30px;\r\n      font-weight: bold;\r\n      color: #333;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/UserCenter/ExpertRelatedInfo"}]}